package sql.c;

public class solidprinciples {
    interface Printer {
        void print();
    }
    interface Fax {
        void fax();
    }
    static class InkPrinter implements Printer {
        public void print() { System.out.println("Ink Printer: Printing document"); }
    }
    public static void main(String[] args) {
        Printer printer = new InkPrinter();
        printer.print();
    }
}

//this is 4th principle interface
// i taken customs has printer fax
