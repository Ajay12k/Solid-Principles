package assign.five;

import java.util.*;

public class ass5 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        LinkedList<String> apps = new LinkedList<>();

        System.out.println("Enter 5 app names:");
        for (int i = 0; i < 5; i++) {
            String app = sc.nextLine();
            apps.remove(app);       
            apps.addFirst(app);     
        }
        System.out.println("recent apps: " + apps);
    }
}
